  booktitle = {Oxford {{Research Encyclopedia}} of {{Psychology}}},
  author = {Mast, Fred W. and Gurtner, Lilla M.},
  date = {2023-03-22},
  publisher = {Oxford University Press},
  doi = {10.1093/acrefore/9780190236557.013.776},
  abstract = {Mental rotation is the ability to mentally represent the hypothetical view of an object rotated away from its actual viewpoint. It can be experimentally tested by a paradigm in which participants judge whether two stimuli are identical or not. The two stimuli are rotated and the size of angle between the two determines how long participants will take to come to a decision. This suggests that mental rotation is a mental process analogous to real rotation. This finding has been of importance for mental imagery research more broadly because (a) it illustrated that, unlike in behavioristic thinking, it is possible to research mental processes in a scientific way, and (b) because it was the foundation of many experiments supporting the similarities between mental imagery and perception, both in terms of brain activation and in terms of computational models.},
  bookauthor = {Mast, Fred W. and Gurtner, Lilla M.},
