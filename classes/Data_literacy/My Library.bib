@article{Ballard1997,
  title = {Deictic Codes for the Embodiment of Cognition},
  author = {Ballard, Dana H. and Hayhoe, Mary M. and Pook, Polly K. and Rao, Rajesh P.N.},
  date = {1997},
  journaltitle = {Behavioral and Brain Sciences},
  volume = {20},
