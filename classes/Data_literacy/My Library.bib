  author = {Bar, M. and Kassam, K. S. and Ghuman, A. S. and Boshyan, J. and Schmidt, A. M. and Dale, A. M. and Hämäläinen, M. S. and Marinkovic, K. and Schacter, D. L. and Rosen, B. R. and Halgren, E.},
  date = {2006},
