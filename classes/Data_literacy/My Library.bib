@article{Payne2020,
  title = {The Evolution of Complex Life and the Stabilization of the {{Earth}} System},
  author = {Payne, Jonathan L. and Bachan, Aviv and Heim, Noel A. and Hull, Pincelli M. and Knope, Matthew L.},
  date = {2020-08-06},
  journaltitle = {Interface Focus},
