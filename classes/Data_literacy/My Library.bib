  keywords = {Dimensions of affordability,Prism of sustainable consumption,Social practice theory SPT,Sufficiency,Sufficiency policy options,Theory of planned behaviour TPB},
  file = {/Users/gurtnerlilla/Zotero/storage/LHFCIDXL/1-s2.0-S0301421519301764-main.pdf}
}

@article{Spektor2024,
