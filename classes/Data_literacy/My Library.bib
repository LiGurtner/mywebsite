@article{Winkelmann2022,
  title = {Social Tipping Processes towards Climate Action: {{A}} Conceptual Framework},
  author = {Winkelmann, Ricarda and Donges, Jonathan F. and Smith, E. Keith and Milkoreit, Manjana and Eder, Christina and Heitzig, Jobst and Katsanidou, Alexia and Wiedermann, Marc and Wunderling, Nico and Lenton, Timothy M.},
