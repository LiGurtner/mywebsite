  file = {/Users/gurtnerlilla/Zotero/storage/2WG6PGAA/3---Climate-change-communication--Challenges--insig_2018_Psychology-and-Clim.pdf;/Users/gurtnerlilla/Zotero/storage/383RR5AA/2---Perceptions-of-climate-change_2018_Psychology-and-Climate-Change.pdf;/Users/gurtnerlilla/Zotero/storage/57YUXD5V/10---Individual-impacts-and-resilience_2018_Psychology-and-Climate-Change.pdf;/Users/gurtnerlilla/Zotero/storage/7DEFUYWQ/8---Environmental-protection-through-societal-change--What_2018_Psychology-a.pdf;/Users/gurtnerlilla/Zotero/storage/8RYH45PZ/5---A-diversity-science-approach-to-climate-c_2018_Psychology-and-Climate-Ch.pdf;/Users/gurtnerlilla/Zotero/storage/ENFA88UG/1---Introduction--Psychology-and-climate-cha_2018_Psychology-and-Climate-Cha.pdf;/Users/gurtnerlilla/Zotero/storage/EZWZQST8/11---Psychological-perspectives-on-community-resilience-an_2018_Psychology-a.pdf;/Users/gurtnerlilla/Zotero/storage/FUCZXBV3/1---Introduction--Psychology-and-climate-cha_2018_Psychology-and-Climate-Cha.pdf;/Users/gurtnerlilla/Zotero/storage/Q23IT8D3/6---Contributions-of-psychology-to-limiting-climate-cha_2018_Psychology-and-.pdf;/Users/gurtnerlilla/Zotero/storage/QFYG5IIB/9---Threats-to-mental-health-and-wellbeing-associat_2018_Psychology-and-Clim.pdf;/Users/gurtnerlilla/Zotero/storage/YZ7EVIEE/4---Social-construction-of-scientifically-grounded-_2018_Psychology-and-Clim.pdf}
