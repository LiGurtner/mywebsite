@article{Heyen2019,
  title = {Drivers and Barriers of Sustainability Transformations: {{A}} Comparison of the “{{Energiewende}}” and the Attempted Transformation to Organic Agriculture in {{Germany}}},
  shorttitle = {Drivers and Barriers of Sustainability Transformations},
  author = {Heyen, Dirk Arne and Wolff, Franziska},
  date = {2019-01-01},
