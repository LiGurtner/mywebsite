@article{Denis1999,
  title = {Scanning Visual Mental Images: {{A}} Window on the Mind.},
  author = {Denis, Michel and Kosslyn, Stephen Michael},
  date = {1999},
  journaltitle = {Cahiers de Psychologie Cognitive/Current Psychology of Cognition},
  volume = {18},
  number = {4},
  pages = {409--465},
  publisher = {CPC},
  location = {France},
  issn = {0249-9185(Print)},
  abstract = {The process of mental scanning corresponds to the systematic shifting of attention over visualized objects. The 1st part of this article focuses on the role of mental scanning as an empirical method to assess the structural properties of the representations that underlie visual mental imagery. One theory of imagery posits that the metric properties of the surfaces of objects are made explicit in visual images. If so, then the time to scan across imaged objects should increase linearly with the distance scanned, and such results have been reported. But these results are controversial and alternative accounts are not compelling. Results from image scanning studies are best interpreted as reflecting the metric properties of imagery representations; imagery uses mechanisms that are used to encode and interpret objects during perception. The 2nd part of this article focuses on the use of image scanning to examine whether verbal descriptions may be used to construct images with structural properties similar to those of images of previously seen objects. Newly constructed images have been shown to possess a structure very much like the representations arising from visual perception. The use of mental scanning paradigm in neuroimaging studies also is considered. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {*Imagery,*Naming,*Neuroanatomy,*Tomography,*Visual Perception,Verbal Communication}
}

