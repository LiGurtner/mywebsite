@article{Brosch2018,
  title = {What We ‘See’ When We Read: {{Visualization}} and Vividness in Reading Fictional Narratives},
  author = {Brosch, Renate},
  date = {2018-08-01},
  journaltitle = {Cortex},
  volume = {105},
  eprint = {28916259},
