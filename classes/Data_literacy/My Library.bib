@article{Bannor2021,
  title = {Extent of Urban Agriculture and Food Security: Evidence from {{Ghana}} and {{India}}},
  shorttitle = {Extent of Urban Agriculture and Food Security},
  author = {Bannor, Richard Kwasi and Sharma, Mohit and Oppong-Kyeremeh, Helena},
  date = {2021-01-08},
