@article{Huppert2019,
  title = {The Development of Children's Preferences for Equality and Equity across 13 Individualistic and Collectivist Cultures},
  author = {Huppert, Elizabeth and Cowell, Jason M. and Cheng, Yawei and Contreras-Ibáñez, Carlos and Gomez-Sicard, Natalia and Gonzalez-Gadea, Maria Luz and Huepe, David and Ibanez, Agustin and Lee, Kang and Mahasneh, Randa and Malcolm-Smith, Susan and Salas, Natalia and Selcuk, Bilge and Tungodden, Bertil and Wong, Alina and Zhou, Xinyue and Decety, Jean},
