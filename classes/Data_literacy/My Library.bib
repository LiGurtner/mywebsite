  keywords = {Cognitive processing,Eye movement recording,Ocular fixation definition,Statistical methods,Temporal fixation threshold},
  file = {/Users/gurtnerlilla/Zotero/storage/8QH4WJ5Y/2003-Defining_the_temporal_threshold_for_ocular_fixation_in_free-viewing_visuocognitive_tasks.pdf}
