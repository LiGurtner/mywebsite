@article{Wynn2019,
  title = {Eye Movements Actively Reinstate Spatiotemporal Mnemonic Content},
  author = {Wynn, Jordana S. and Shen, Kelly and Ryan, Jennifer D.},
  date = {2019-06-01},
  journaltitle = {Vision (Switzerland)},
  volume = {3},
