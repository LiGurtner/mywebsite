@book{Bamberg2018,
  title = {Environmental Protection through Societal Change: {{What}} Psychology Knows about Collective Climate Action—and What It Needs to Find Out},
  author = {Bamberg, Sebastian and Rees, Jonas H. and Schulte, Maxie},
  date = {2018},
  journaltitle = {Psychology and Climate Change: Human Perceptions, Impacts, and Responses},
  number = {2015},
  publisher = {Elsevier Inc.},
  doi = {10.1016/B978-0-12-813130-5.00008-4},
  abstract = {Global and societal challenges such as climate change cannot be solved by focusing on single actors and individual behavior change alone but are inherently social problems. The current chapter therefore focuses on the group dynamics involved in the context of climate protection. More specifically, we examine psychological factors underlying individuals’ engagement in collective climate protection. We begin by briefly outlining an influential model developed in the context of collective protest, the social identity model of collective action and its recent modifications. We then develop a preliminary model of collective climate action, summarize research on how individuals become activists in the first place, and discuss practical interventions on how to get people engaged. Finally, we take a closer look at the various psychological outcomes of participation in collective action. While engagement is costly, e.g., with regard to time, there is also growing evidence that ongoing participation in collective action can evoke feelings of empowerment and stimulate personal development. A psychology of collective climate action can lay the foundations for an investigation of-and hopefully inspire research on-the genuinely social transformative processes in the context of climate protection.},
  isbn = {978-0-12-813130-5},
  pagetotal = {185-213},
